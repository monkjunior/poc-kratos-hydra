{
  "env": "production",
  "baseUrl": "http://127.0.0.1:4455",
  "kratos": {
    "host": "oathkeeper:4455",
    "scheme": "http",
    "debug": true,
    "publicBasePath": "/.ory/kratos/public"
  },
  "hydra": {
    "browserURL": "http://127.0.0.1:4455/.ory/hydra/public",
    "public": {
      "host": "hydra:4444",
      "schemes": ["http"],
      "basePath": "/"
    },
    "admin": {
      "host": "hydra:4445",
      "schemes": ["http"],
      "basePath": "/"
    },
    "client": {
      "id": "authctl-client",
      "secret": "secret",
      "grantTypes": [
        "authorization_code",
        "refresh_token"
      ],
      "responseTypes": [
        "code",
        "id_token"
      ],
      "callbacksURL": ["http://127.0.0.1:4455/callback"],
      "scopes": "openid offline"
    }
  },
  "log": {
    "mode": "dev",
    "level": "debug"
  }
}
